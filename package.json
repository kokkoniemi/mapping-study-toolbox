{
  "name": "mapping-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "migrate": "node_modules/.bin/sequelize db:migrate"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "./public/**/*"
    ],
    "scripts": [
      "./node_modules/*",
      "./routes/*",
      "./models/*",
      "./migrations/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-arm64",
      "node18-win-x64"
    ],
    "compress": "GZip"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "core-js": "^3.37.0",
    "express": "^4.19.2",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "sequelize-cli": "^6.6.2"
  }
}
